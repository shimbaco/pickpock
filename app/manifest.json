{
  "manifest_version": 2,
  "name": "PickPock",
  "description": "Pick your unread article on Pocket and open it in new tab.",
  "version": "0.0.1",
  "permissions": [
    "https://getpocket.com/"
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "image/icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
